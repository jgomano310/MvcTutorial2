@{

    <title>lista usuarios</title>

    @using (Html.BeginForm()){


        <select name="lista">



            @foreach (string usuario in Model)
            {

                <option>
                    @usuario
                </option>
            }
        </select>

        <br/>

        if (ViewBag.Nombre != null)
        {
            <h4> usuario seleccionado @ViewBag.Nombre</h4>

        }

        


        <br />
        <button type="submit">seleccionar usuario</button>


    }
    

    @Html.ActionLink("regresar","Index");
}
